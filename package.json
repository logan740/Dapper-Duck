{
  "name": "dapper-duck-web",
  "version": "1.0.0",
  "description": "A fun blockchain-integrated game where you play as a dapper duck collecting meme snacks while dodging FUD bags",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:testnet": "hardhat run scripts/deploy.js --network abstract",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network abstractMainnet"
  },
  "dependencies": {
    "@abstract-foundation/agw-client": "^1.9.0",
    "@abstract-foundation/agw-react": "^1.9.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.85.5",
    "@wagmi/connectors": "^5.9.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.36.0",
    "wagmi": "^2.16.9"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^4.9.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "hardhat": "^2.26.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "keywords": [
    "game",
    "blockchain",
    "web3",
    "abstract",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "Dapper Duck Team",
  "license": "MIT"
}
